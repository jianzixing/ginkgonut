{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "outDir": "/dist",
    "allowJs": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "noEmit": true,
    "removeComments": true,
    "jsx": "preserve",
    "jsxFactory": "Ginkgo",
    "reactNamespace": "Ginkgo"
  },
  "exclude": [
    "node_modules"
  ]
}
